<?xml version="1.0"?>
<package >
  <metadata>
    <id>ServiceStack.Discovery.Redis</id>
    <version>$version$</version>
    <authors>Richard J. Safier (@rsafier)</authors>
    <description>ServiceStack plugin that supports service discovery and request uri resolution using Redis</description>
    <projectUrl>https://github.com/rsafier/ServiceStack.Discovery.Redis</projectUrl>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <iconUrl>https://servicestack.net/img/logo-32.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>servicestack plugin discovery servicediscovery microservices redis netstandard20</tags>
    <releaseNotes>See github project for details</releaseNotes>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="ServiceStack.Server" version="5.2" />
      </group>
      <group targetFramework=".netstandard2.0">
        <dependency id="ServiceStack.Server" version="5.2" />
      </group>
    </dependencies>
    <files>
      <file src="lib\**" target="lib/{framework name}[{version}]" />
    </files>
  </metadata>
</package>